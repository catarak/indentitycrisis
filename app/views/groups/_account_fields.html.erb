<div class="form-group">
  <h3>New Account</h3>
</div>
<div class="form-group">
  <%= f.label :name, value: "Twitter Username", class: "col-lg-3 control-label" %>
  <div class="col-lg-9">
    <%= f.text_field :name, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :application_token, class: "col-lg-3 control-label" %>
  <div class="col-lg-9">
    <%= f.text_field :application_token, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :application_token_secret, class: "col-lg-3 control-label" %>
  <div class="col-lg-9">
    <%= f.text_field :application_token_secret, class: "form-control" %>
  </div>
<div class="form-group">
  <%= f.label :access_token, class: "col-lg-3 control-label" %>
  <div class="col-lg-9">
    <%= f.text_field :access_token, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :access_token_secret, class: "col-lg-3 control-label" %>
  <div class="col-lg-9">
    <%= f.text_field :access_token_secret, class: "form-control" %>
  </div>
</div>