<div class="row">
  <div class="col-md-6">
    <ul>
      <% @accounts.each do |account| %>
        <li><%= link_to account.name, account_path(account) %></li>
      <% end %>
    </ul>
  </div>
  <div class="col-md-6">
    <% @members.each do |member| %>
      <li>
        <%= member.name %>
        <% if member.memberships.find_by(group_id: @group.id).admin? %>
            (Admin)
        <% end %>
      </li>
    <% end %>
  </div>
</div>